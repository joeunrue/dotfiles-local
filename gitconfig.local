[user]
  name = Saxon Unrue
  email = saxon@unrue.com
[alias]
  aa = add --all
  amend = commit --amend
  b = branch
  ci = commit
  co = checkout
  dc = diff --cached
  di = diff
  dh = diff HEAD
  dh1 = diff HEAD~1
  la = !git l --all
  pf = push --force-with-lease
  st = status
  prune-stale = !git remote prune origin && git branch -vv | grep ': gone]' | awk '{print $1}' | xargs git branch -D
[branch]
  autosetuprebase = always
[color]
  ui = auto
[format]
  pretty = %Cblue%h%Creset %Cgreen[%ar]%Creset (%an) %s
[log]
  decorate = short
[push]
  default = upstream
[commit]
  gpgsign = true
[user]
  signingkey = F9BEFB7CE7AFB88D
